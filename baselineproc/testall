#!/bin/bash
TESTDIR="testout"
BASEDIR="baseline"
rm $TESTDIR/*
rm testresults
./runall $TESTDIR
for filename in $TESTDIR/*.at; do
	if [[ ! -e "$filename" ]]; then continue; fi
	diff $filename $BASEDIR/"$(basename "$filename" .at).at" >> testresults
done
echo "-------------------------------------------------------------------------------------------"
echo "-------------------------------------------------------------------------------------------"
echo "-------------------------------------------------------------------------------------------"
echo "-------------------------------------------------------------------------------------------"
echo "-------------------------------------------------------------------------------------------"
echo "-------------------------------------------------------------------------------------------"
echo "-------------------------------------------------------------------------------------------"
cat testresults
